{
  "version": 1,
  "include": [
    "public/**"
  ],
  "exclude": [
    "**/.DS_Store",
    "**/node_modules/**",
    "**/.git/**",
    "**/.vscode/**",
    "**/*.log",
    "**/.env",
    "**/__pycache__/**",
    "**/.cache/**",
    "**/build/**",
    "**/dist/**",
    "**/.venv/**",
    "**/venv/**"
  ],
  "output": {
    "maxFileSize": "10MB",
    "topFilesLength": 20
  }
}
